{
  "Class": {
    "prefix": "class",
    "body": ["class ${1:Name}", "{", "\t$0", "}"],
    "description": "Class"
  },
  "Console.WriteLine": {
    "prefix": "cw",
    "body": "Console.WriteLine($0);",
    "description": "Console.WriteLine"
  },
  "do...while loop": {
    "prefix": "do",
    "body": ["do", "{", "\t$0", "} while (${1:cond});"],
    "description": "do...while loop"
  },
  "Else statement": {
    "prefix": "else",
    "body": ["else", "{", "\t$0", "}"],
    "description": "Else statement"
  },
  "Enum": {
    "prefix": "enum",
    "body": ["enum ${1:Name}", "{", "\t$0", "}"],
    "description": "Enum"
  },
  "Foreach statement": {
    "prefix": "foreach",
    "body": [
      "foreach (${1:var} ${2:item} in ${3:collection})",
      "{",
      "\t$0",
      "}"
    ],
    "description": "Foreach statement"
  },
  "for loop": {
    "prefix": "for",
    "body": ["for ($1)", "{", "\t$0", "}"],
    "description": "for loop"
  },
  "if statement": {
    "prefix": "if",
    "body": ["if (${1:cond})", "{", "\t$0", "}"],
    "description": "if statement"
  },
  "Interface": {
    "prefix": "interface",
    "body": ["interface I${1:Name}", "{", "\t$0", "}"],
    "description": "Interface"
  },
  "Namespace": {
    "prefix": "namespace",
    "body": ["namespace ${1:Name}", "{", "\t$0", "}"],
    "description": "Namespace"
  },
  "#if": {
    "prefix": "ifd",
    "body": ["#if ${1:true}", "\t$0", "#endif"],
    "description": "#if"
  },
  "#region": {
    "prefix": "region",
    "body": ["#region ${1:Name}", "\t$0", "#endregion"],
    "description": "#region"
  },
  "Struct": {
    "prefix": "struct",
    "body": ["struct ${1:Name}", "{", "\t$0", "}"],
    "description": "Struct"
  },
  "Switch statement": {
    "prefix": "switch",
    "body": ["switch (${1:cond})", "{", "\t$0", "}"],
    "description": "Switch statement"
  },
  "Try catch finally": {
    "prefix": "try",
    "body": [
      "try",
      "{",
      "\t$1",
      "}",
      "catch (${2:System.Exception})",
      "{",
      "\t$0",
      "\tthrow;",
      "}",
      "finally",
      "{",
      "\t$0",
      "}"
    ],
    "description": "Try catch"
  },
  "Using statement": {
    "prefix": "using",
    "body": ["using (${1:resource})", "{", "\t$0", "}"],
    "description": "Using statement"
  },
  "While loop": {
    "prefix": "while",
    "body": ["while (${1:cond})", "{", "\t$0", "}"],
    "description": "While loop"
  },
  "Function": {
    "prefix": "fn",
    "body": ["${1:public} ${2:void} ${3:Function}($4)", "{", "\t$0", "}"],
    "description": "Create a function"
  },
  "Function async": {
    "prefix": "afn",
    "body": [
      "${1:public} async ${2:Task}<${3:object}> ${4:FunctionAsync}($5)",
      "{",
      "\t$0",
      "}"
    ],
    "description": "Create an async function"
  }
}
