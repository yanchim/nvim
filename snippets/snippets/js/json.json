{
  "VSCode snippet template": {
    "prefix": "snippet",
    "body": [
      "\"${1:snippet name}\": {",
      "\t\"prefix\": \"${2:prefix}\",",
      "\t\"body\": [",
      "\t\t\"$3\"",
      "\t],",
      "\t\"description\": \"${4:desc}\"",
      "},"
    ],
    "description": "Log output to console"
  }
}
